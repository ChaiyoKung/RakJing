{
  "expo": {
    "name": "RakJing",
    "slug": "RakJing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DD0426"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFCE"
      },
      "package": "com.chaiyokung.RakJing"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "firebaseConfig": {
        "apiKey": "{Replace with your firebase config}",
        "authDomain": "{Replace with your firebase config}",
        "databaseURL": "{Replace with your firebase config}",
        "projectId": "{Replace with your firebase config}",
        "storageBucket": "{Replace with your firebase config}",
        "messagingSenderId": "{Replace with your firebase config}",
        "appId": "{Replace with your firebase config}"
      }
    }
  }
}